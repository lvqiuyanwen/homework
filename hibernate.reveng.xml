			<generator class="increment"></generator>
		</primary-key>
	</table>
	<table name="t_book" catalog="giel" class="com.home.model.Book">
		<primary-key>
			<generator class="increment"></generator>
		</primary-key>
	</table>
	<table name="t_order" catalog="giel" class="com.home.model.Order">
		<primary-key>
			<generator class="increment"></generator>
		</primary-key>
	</table>
	<table name="t_user" catalog="giel" class="com.home.model.User">
		<primary-key>
			<generator class="increment"></generator>
		</primary-key>
	</table>
<table name="t_write" catalog="giel" class="com.home.model.Write">
	<primary-key>
		<generator class="increment"></generator>
	</primary-key>
</table>
</hibernate-reverse-engineering>